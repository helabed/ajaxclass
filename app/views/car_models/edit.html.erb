<h1>Editing car_model</h1>

<% form_for(@car_model) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :model %><br />
    <%= f.text_field :model %>
  </p>
  <p>
    <%= f.label :car_make_id %><br />
    <%= f.text_field :car_make_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @car_model %> |
